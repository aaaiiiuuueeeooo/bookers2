<%= render 'list', {user:@user,book:@book} %>

<h1>Books</h1>

<% @books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
    <%= image_tag book.user.get_profile_image(100,100) %>
  <% end %>


<p>Title</p>
<span><%= book.title %></span>

<p>Opinion</p>
<span><%= book.body %></span>
</div>
<% end %>